
<div = "row">
  <% board_products.each do |board_product| %>
  	<% if board_product.product %>
	    <% if board_product.product.variants.empty? %>
	      <%= render "spree/admin/board_products/board_product_with_no_variants_tile", board_product: board_product %>
	    <% else %>
	      <%= render "spree/admin/board_products/board_product_with_variants_tile", board_product: board_product %>
	    <% end %>
	  <% end %>
  <% end %>
</div>


